                    android:layout_toLeftOf="@id/llsubmit"
                    android:layout_width="fill_parent"
                    android:layout_height="40dp"
                    android:layout_alignTop="@+id/llsubmit"
                    android:background="@drawable/comm_input_bg"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/input_style_other"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dip"
                        android:singleLine="true"
                        android:text="短信验证码:" />

                    <EditText
                        android:id="@+id/llet_codekey"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dip"
                        android:background="@null"
                        android:maxLength="6"
                         android:numeric="integer"
                        android:maxLines="1"
                        android:hint="输入验证码"
                        style="@style/editText_style"
                         />
                </LinearLayout>
        
            </RelativeLayout>
                <LinearLayout
             		android:visibility="gone"
                  android:id="@+id/lllin_msg"
                  android:layout_marginLeft="15dip"
                  android:layout_marginRight="11dip"
                  android:layout_marginTop="20dip"
                  android:layout_width="fill_parent"
                  android:layout_height="wrap_content"
                  android:orientation="vertical"
                 > 
                <TextView 
                    android:id="@+id/lltx_tishi"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="重要提示："
                    android:textColor="#3f3f3f"
                    android:textSize="15sp"
                    />
                      <TextView 
                     android:id="@+id/lltishi_str"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:text=""
                     android:textSize="14sp"
                     android:textColor="#3f3f3f"
                     android:ellipsize="none"
					 android:maxLines="100"
 					 android:scrollHorizontally="false"
                    />
                      </LinearLayout>
             
            <!-- end -->
                <Button
                    android:layout_marginTop="10dp"
                    android:id="@+id/confirm_button"
                    style="@style/next_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginBottom="25dp"
                    android:background="@drawable/button_next_selector"
                    android:gravity="center"
                    android:text="确认并提交" />
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>

</LinearLayout>